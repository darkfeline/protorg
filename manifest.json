{
  "manifest_version": 2,

  "name": "protorg",
  "description": "org-protocol",
  "version": "0.3.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "commands": {
    "store-link": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      },
      "description": "store-link"
    },
    "capture": {
      "suggested_key": {
        "default": "Ctrl+Y"
      },
      "description": "capture"
    }
  },
  "background": {
    "scripts": ["menu.js"]
  }
}
